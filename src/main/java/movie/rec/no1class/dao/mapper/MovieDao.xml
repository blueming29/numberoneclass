<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="movie.rec.no1class.dao.MovieDao">
	<select id="list" resultType="movie.rec.no1class.dto.MovieDto">
		SELECT * FROM MOVIE_LIST WHERE MOVIE_POINT > 9.5 ORDER BY CAST(MOVIE_POINT AS NUMBER) DESC
	</select>


</mapper>